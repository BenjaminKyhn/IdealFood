<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.Scene?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>
<?import javafx.stage.Stage?>

<Stage title="IdealFood" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="GUI.Main">
    <scene>
        <Scene>
            <AnchorPane prefHeight="546.0" prefWidth="822.0">
            <children>
               <ImageView fitHeight="580.0" fitWidth="823.0" pickOnBounds="true" preserveRatio="true">
                  <image>
                     <Image url="https://cdn.shopify.com/s/files/1/1595/7397/products/2414_Fresh_Mixed_Fruits_1500_Pc_Jigsaw_Puzzle_by_Colorlux_1800x.jpg?v=1551122385" />
                  </image>
               </ImageView>
               <Button fx:id="storeIngredient" layoutX="321.0" layoutY="507.0" mnemonicParsing="false" onAction="#popupStoreIngredient" prefHeight="70.0" prefWidth="182.0" text="STORE INGREDIENT">
                  <font>
                     <Font size="16.0" />
                  </font>
               </Button>
               <Button fx:id="findRecipeButton" alignment="CENTER" layoutX="295.0" layoutY="98.0" mnemonicParsing="false" onAction="#popupChooseIngredients" prefHeight="61.0" prefWidth="226.0" text="FIND RECIPE" textAlignment="CENTER">
                  <font>
                     <Font name="System Bold" size="18.0" />
                  </font>
               </Button>
            </children></AnchorPane>
        </Scene>
    </scene>
</Stage>
